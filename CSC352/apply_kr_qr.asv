function D = apply_kr_qr(Q,Q_hat,B,C)
%  Q: cell array of factors QR's Q 
%  Q_hat: cell array of pairwise Q
%  B: cell array of factors on RHS
%  C: RHS transpose
%  D: QtB
d = length(Q);
n = size(Q{1},2);

%update RHS with Q'
for i = 1:d
    B{i} = Q{i}' * B{i};
end
D = eye(d,n);


%update pairwise
j = d-1;
for i = 1:d-1
    B{i+1} = khatrirao(B{i}, B{i+1});
    
    j = j - 1;
end
D = D * C';




end
